<?xml version="1.0" encoding="utf-8"?>
<manifest package="test.wightview"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <!--android:icon="@mipmap/ic_launcher"-->
    <!--android:label="@string/app_name"-->
    <application
        android:allowBackup="true"
        android:icon="${icon}"
        android:label="${app_name}"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".item.ItemViewActivity"></activity>
        <activity android:name=".numeditext.EditTextActivity"></activity>
        <activity android:name=".customview.CustomActivity"></activity>
        <activity android:name=".adapter.CommonCheckActivity"></activity>
        <activity android:name=".fragment.FragmentMainActivity"></activity>
        <activity android:name=".overtextview.OverTextViewActivity"></activity>
        <activity android:name=".validedittext.TextViewActivity"></activity>
        <activity android:name=".image.ImageActivity"></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest>
